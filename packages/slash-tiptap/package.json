		"cmdk",
		"tiptap slash"
	],
	"type": "module",
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.18.2",
		"@poltion/typescript-config": "workspace:*",
		"@changesets/cli": "^2.29.5",
		"@types/node": "^24.0.15",
		"@types/react": "catalog:",
		"tsup": "^8.5.0",
